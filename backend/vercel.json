{
  "version": 2,
  "name": "fruit-zone-backend",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["./**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
